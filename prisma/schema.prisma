generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model issue {
  id               Int          @id @default(autoincrement())
  title            String       @db.VarChar(225)
  description      String       @db.Text
  status           issue_status @default(OPEN)
  createdAt        DateTime     @default(now())
  updatedAt        DateTime
}

enum issue_status {
  OPEN
  IN_PROGRESS
  CLOSED
}

